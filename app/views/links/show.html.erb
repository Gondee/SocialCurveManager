<% provide(:title, "Link") %>
<% provide(:headline, "Link") %>
<% provide(:icon, "unchecked") %>
<p id="notice"><%= notice %></p>

<p>
  <strong>Url:</strong>
  <%= @link.url %>
</p>

<p>
  <strong>Date:</strong>
  <%= @link.date %>
</p>

<p>
  <strong>Used:</strong>
  <%= @link.used %>
</p>

<%= link_to 'Edit', edit_link_path(@link) %> |
<%= link_to 'Back', links_path %>
