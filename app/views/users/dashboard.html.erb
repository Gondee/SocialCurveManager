<% provide(:title, "Dashboard") %>
<% provide(:headline, "My Dashboard") %>
<% provide(:icon, "dashboard") %>

<% if current_user.admin? %>

<%= link_to "Assign Links", linkdistribution_path, class: "btn btn-lg btn-primary" %>

    <%= render 'dashboard/admin' %>

<% elsif current_user.publisher %>
    <%= render 'dashboard/publisher' %>
<% else %>
    <%= render 'dashboard/user' %>
<% end %>